{% extends 'base.html.twig' %} 
{% block title %}{{title}}{% endblock %} 

{% block body %} 
    <h1>{{title}}</h1>
    {{ form_start(formCategory)}}
    <div class="form-group">
        {{form_row(formCategory.name, { 'label' : 'Titre', 'attr' : {'class' : 'form-control'} })}}
    </div>
    <div class="form-group">
        {{form_row(formCategory.Sauvegarder, { 'attr' : {'class' : 'btn btn-success'} })}}
    </div>
{{ form_end(formCategory)}} 

{% endblock %}
